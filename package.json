{
  "name": "paysis-tests-js",
  "version": "1.0.0",
  "description": "Babel + Jest + Axios API tests examples for Paysis project",
  "scripts": {
    "test": "jest",
    "format": "prettier ./ --write --list-different",
    "lint": "prettier ./ --check"
  },
  "keywords": [],
  "author": "Dmitrii Bormotov",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "7.22.5",
    "axios": "1.4.0",
    "babel-plugin-module-resolver": "5.0.0",
    "dotenv": "16.3.0",
    "jest": "29.5.0",
    "prettier": "2.8.8"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ],
    "transform": {
      "\\.[jt]sx?$": "babel-jest"
    },
    "moduleDirectories": [
      "node_modules",
      "<rootDir>"
    ]
  }
}
